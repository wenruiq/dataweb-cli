{
  "name": "dataweb",
  "version": "1.0.0",
  "description": "Sync Swagger/OpenAPI specs to Bruno collections",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "dataweb": "./dist/dataweb"
  },
  "scripts": {
    "dev": "bun --hot src/cli.ts",
    "build": "bun run scripts/build.ts",
    "build:binary": "bun build --compile --minify --sourcemap src/cli.ts --outfile dist/dataweb",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "check": "bunx @biomejs/biome check .",
    "check:write": "bunx @biomejs/biome check --write .",
    "format": "bunx @biomejs/biome format --write .",
    "prepare": "cp scripts/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@usebruno/cli": "^3.0.2",
    "commander": "^14.0.2"
  }
}
